{
  "extends": "./config/tsconfig.base.json",
  "compilerOptions": {
    "outDir": "build",
    "baseUrl": "src",
    "declaration": true,
    "emitDeclarationOnly": true,
    "build": false,
    "composite": false,
    "sourceMap": false
  },
  "include": [
    "src/components",
    "src/interfaces",
    "src/utils",
    "src/module.ts"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}
